group_by(.version) | sort_by(first.version) | reverse | 
  map({
    version:first.version,
    organData: group_by(.nameOrgan) | sort_by(first.alt) | map({
      label: first.nameOrgan,
      icon: first.image,
      cards: map({
        label: .name,
        alt: .alt,
        fileUrl: .threeDimImage,
        sourceDataUrl: .url,
        files: []
      })
    })
  })
